{
  "name": "svg-to-inline",
  "version": "1.0.0-alpha.1",
  "description": "Web Component to fetch an external SVG file and use it inline.",
  "homepage": "https://tiagoporto.github.io/svg-to-inline",
  "keywords": [
    "svg",
    "inline",
    "fetch",
    "include",
    "inject",
    "web component",
    "component"
  ],
  "author": {
    "name": "Tiago Porto",
    "url": "http://tiagoporto.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiagoporto/svg-to-inline.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 20",
    "npm": ">= 10"
  },
  "type": "module",
  "customElements": "custom-elements.json",
  "scripts": {
    "analyze": "custom-elements-manifest analyze --litelement --exclude demo dist storybook-static \"**/*.(story|stories).*\"",
    "prepare": "husky",
    "start": "tsc --project tsconfig.build.json && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"web-dev-server\"",
    "build": "tsc --project tsconfig.build.json && npm run analyze",
    "checks": "npm run check-docs && npm run check-formatting && npm run check-typing && npm run lint",
    "check-docs": "remark --frail .",
    "check-formatting": "prettier --check --ignore-unknown \"**/*\"",
    "formatting:fix": "npm run check-formatting -- --write",
    "check-typing": "tsc --project tsconfig.json",
    "lint": "eslint . --ext js,mjs,ts,html --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "test": "",
    "test:watch": "",
    "docs": "",
    "docs:build": "",
    "start:site": "storybook dev -p 6006",
    "build:site": "storybook build"
  },
  "dependencies": {
    "lit": "^3.2.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@custom-elements-manifest/analyzer": "^0.10.3",
    "@open-wc/eslint-config": "^12.0.3",
    "@storybook/addon-essentials": "^8.4.2",
    "@storybook/blocks": "^8.4.2",
    "@storybook/test": "^8.4.2",
    "@storybook/web-components": "^8.4.2",
    "@storybook/web-components-vite": "^8.4.2",
    "@tiagoporto/prettier-config": "^1.0.0-alpha.2",
    "@tiagoporto/remark-config": "^1.0.0-alpha.1",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@web/dev-server": "^0.4.6",
    "concurrently": "^9.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "storybook": "^8.4.2",
    "typescript": "^5.6.3"
  }
}
